<!--pages/conversation/conversation.wxml-->
<view class='top'>
  <text class='top-text'>会话</text>
</view>
<view class='person' bindtap='toChat'>
  <view class='person-list' wx:for="{{list}}" wx:key="index">
    <image src="{{item.img_url}}"></image>
    <text class='person-name'>{{item.name}}</text>
  </view>
</view> 